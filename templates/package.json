{
  "name": "jam3-new-app",
  "version": "0.0.1",
  "description": "Jam3 new app",
  "private": true,
  "scripts": {
    "start": "npm-run-all -p watch-css start-js",
    "start-js": "react-app-rewired start",
    "release": "cross-env NODE_ENV=production npm-run-all generate-doc build-css build-js",
    "build": "npm run release",
    "build-css": "node-sass-chokidar --include-path ./src/style --include-path ./node_modules src/ -o src/",
    "build-js": "react-app-rewired build",
    "test": "react-app-rewired test --env=jsdom",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./src/style --include-path ./node_modules src/ -o src/ --watch --recursive",
    "js-lint": "eslint './src/**/*.js' -c ./.eslintrc --quiet --ignore-pattern .gitignore",
    "sass-lint": "stylelint './src/**/*.scss'",
    "linters": "npm run js-lint && npm run sass-lint",
    "precommit": "echo 'Pre-commit checks...' && lint-staged",
    "prepush": "echo 'Pre-push checks...' && npm run linters",
    "storybook": "start-storybook -p 9001 -c .storybook -s src",
    "component": "node scripts/component.js",
    "stateless-component": "node scripts/component.js --stateless",
    "audit-nsp": "nsp check",
    "audit-snyk": "snyk auth [token] && snyk test",
    "audit": "npm run audit-nsp",
    "postinstall": "npm-run-all audit build-css",
    "dependency-report": "node scripts/dependencies-report",
    "svg-component": "svgr",
    "generate-doc": "node scripts/generate-doc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Jam3/prj-fb-messenger-redesign.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Jam3/prj-fb-messenger-redesign/issues"
  },
  "dependencies": {
    "bowser": "1.9.2",
    "classnames": "2.2.5",
    "fullscreen-handler": "0.0.2",
    "gsap": "1.20.4",
    "gsap-promisify": "1.0.0",
    "history": "4.7.2",
    "howler": "2.0.9",
    "lodash.debounce": "4.0.8",
    "mobile-detect": "1.4.1",
    "normalize.css": "8.0.0",
    "react": "^16.2.0",
    "react-background-video-player": "1.0.1",
    "react-dom": "^16.2.0",
    "react-loadable": "5.3.1",
    "react-redux": "5.0.7",
    "react-router-dom": "4.2.2",
    "react-router-redux": "5.0.0-alpha.9",
    "redux": "3.7.2"
  },
  "devDependencies": {
    "@storybook/addon-actions": "3.3.14",
    "@storybook/react": "3.3.14",
    "babel-eslint": "8.0.2",
    "chalk": "2.3.2",
    "cross-env": "^5.1.1",
    "eslint": "4.11.0",
    "eslint-config-postcss": "^2.0.2",
    "eslint-config-prettier": "2.8.0",
    "eslint-config-react-app": "^2.0.1",
    "eslint-plugin-flowtype": "^2.39.1",
    "eslint-plugin-import": "2.9.0",
    "eslint-plugin-jsx-a11y": "6.0.3",
    "eslint-plugin-prettier": "^2.3.1",
    "eslint-plugin-promise": "3.6.0",
    "eslint-plugin-react": "7.5.1",
    "eslint-plugin-standard": "3.0.1",
    "husky": "^0.14.3",
    "imagemin": "5.3.1",
    "imagemin-gifsicle": "5.2.0",
    "imagemin-jpegtran": "5.0.2",
    "imagemin-pngquant": "5.0.1",
    "imagemin-svgo": "6.0.0",
    "license-checker": "16.0.0",
    "lint-staged": "^4.3.0",
    "maxstache": "1.0.7",
    "minimist": "1.2.0",
    "mkdirp": "0.5.1",
    "node-sass-chokidar": "0.0.3",
    "npm-run-all": "4.1.2",
    "nsp": "3.2.1",
    "prettier": "1.8.2",
    "prop-types": "15.6.1",
    "react-app-rewire-eslint": "0.2.3",
    "react-app-rewire-hot-loader": "1.0.0",
    "react-app-rewire-imagemin-plugin": "1.0.1",
    "react-app-rewired": "1.4.1",
    "react-hot-loader": "4.0.0",
    "react-scripts": "1.1.1",
    "sassdoc": "2.5.0",
    "snyk": "1.69.10",
    "stats.js": "0.17.0",
    "stylelint": "8.2.0",
    "svg-inline-loader": "0.8.0",
    "svgr": "1.9.0",
    "webpack-visualizer-plugin": "0.1.11",
    "why-did-you-update": "0.1.1"
  },
  "lint-staged": {
    "gitDir": "../",
    "linters": {
      "private/prj-frontend/src/**/*.{js,json,scss}": [
        "prettier --write",
        "git add"
      ]
    }
  },
  "browserslist": [
    "Explorer >= 10",
    "Safari >= 9",
    "Chrome >= 30",
    "Firefox >= 40",
    "iOS >= 9"
  ]
}
